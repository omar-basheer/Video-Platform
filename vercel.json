{
  "version": 2,
  "builds": [
    { "src": "manage.py", "use": "@vercel/python" }
    // { "src": "video_platform/wsgi.py", "use": "@vercel/python" }
  ],
  "routes": [
    // { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "video_platform/wsgi.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "video_platform.settings",
    "STATIC_URL": "https://storage.googleapis.com/vpapp_videos/static/",
    "MEDIA_URL": "https://storage.googleapis.com/vpapp_videos/media/"
  }
}
 